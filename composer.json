{
    "name": "hshn/npm-bundle",
    "description": "Handle asset dependencies and scripts with npm",
    "keywords": ["npm"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Shota Hoshino",
            "email": "sht.hshn@gmail.com"
        }
    ],
    "require": {
        "PHP": "^5.6.0 || ^7.0.0",
        "symfony/http-kernel": "^2.8.0 || ^3.4.0",
        "symfony/console": "^2.8.0 || ^3.4.0",
        "symfony/config": "^2.8.0 || ^3.4.0",
        "symfony/dependency-injection": "^2.8.0 || ^3.4.0",
        "symfony/process": "^2.8.0 || ^3.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.27",
        "symfony/framework-bundle": "^2.8.0 || ^3.4.0",
        "symfony/phpunit-bridge": "^2.8.0 || ^3.4.0"
    },
    "autoload": {
        "psr-4": { "Hshn\\NpmBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Hshn\\NpmBundle\\": "test" }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
